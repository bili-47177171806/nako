{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "nightcord-nako",
  "main": "src/index.ts",
  "compatibility_date": "2026-02-10",
  "ai": {
    "binding": "AI"
  },
  "vectorize": [
    {
      "binding": "VECTORIZE",
      "index_name": "sticker-embeddings"
    }
  ],
  "d1_databases": [
    {
      "binding": "AUTH_DB",
      "database_name": "sekai_pass_db",
      "database_id": "YOUR_AUTH_DATABASE_ID"
    },
    {
      "binding": "DB",
      "database_name": "pjsekai_db",
      "database_id": "YOUR_STATS_DATABASE_ID"
    }
  ],
  "vars": {
    "ENVIRONMENT": "production"
  }
  // Uncomment and configure your custom domain
  // "routes": [
  //   {
  //     "pattern": "your-domain.com",
  //     "custom_domain": true
  //   }
  // ]
}
